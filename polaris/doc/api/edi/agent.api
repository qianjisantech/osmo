syntax = "v1"

info
(
    title:   "discovery模块"
    desc:    "discovery模块接口定义"
    author:  "philip.shen"
    email:   "philip.shen@jtexpress.com"
    version: "0.0.1"
)

type (
    EdiAgentInfoSyncReq {
        Id string  `json:"id"`
        Name string `json:"name"`
        Status string `json:"status"`
        StartTime string `json:"start_time"`
        EndTime string `json:"end_time"`
        FailReason string `json:"fail_reason,optional"`
    }
    EdiAgentInfoSyncResp {
        Success bool `json:"success"`
        Message string `json:"message"`
    }
)

@server(
    group: edi
    prefix: /polaris/edi/agent
)
service polaris-api {
    @handler EdiAgentInfoSync
    post /info/sync (EdiAgentInfoSyncReq) returns (EdiAgentInfoSyncResp)
}