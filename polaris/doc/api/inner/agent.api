syntax = "v1"

info
(
    title:   "discovery模块"
    desc:    "discovery模块接口定义"
    author:  "philip.shen"
    email:   "philip.shen@jtexpress.com"
    version: "0.0.1"
)

type (
    InnerAgentInfoSyncReq {
        Id string  `json:"id"`
        Name string `json:"name"`
        Status string `json:"status"`
        StartTime string `json:"start_time"`
        EndTime string `json:"end_time"`
        FailReason string `json:"fail_reason"`
    }
    InnerAgentInfoSyncResp {
        Success bool `json:"success"`
        Message string `json:"message"`
    }
)

@server(
    group: inner
    prefix: /polaris/inner/agent
)
service polaris-api {
    @handler InnerAgentInfoSync
    post /info/sync (InnerAgentInfoSyncReq) returns (InnerAgentInfoSyncResp)
}